{
  "chat.commandCenter.enabled": false,
  "editor.cursorBlinking": "blink",
  "editor.cursorStyle": "block",
  "editor.fontFamily": "JetBrainsMono Nerd Font,JetBrainsMono NF,JetBrainsMono Nerd Font Mono,JetBrainsMono NFM",
  "editor.fontLigatures": false,
  "editor.fontSize": 18,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "trailing",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.tabCompletion": "off",
  "editor.rulers": [
    120
  ],
  "editor.scrollBeyondLastLine": true,
  "editor.wordWrapColumn": 120,
  "elixirLS.enableTestLenses": true,
  "elixirLS.fetchDeps": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "onFocusChange",
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "haskell.manageHLS": "GHCup",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font,JetBrainsMono NF,JetBrainsMono Nerd Font Mono,JetBrainsMono NFM",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.sendKeybindingsToShell": false,
  "vim.cursorStylePerMode.insert": "line",
  "vim.useSystemClipboard": true,
  "window.autoDetectColorScheme": true,
  "window.commandCenter": false,
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "GitHub Dark",
  "workbench.iconTheme": "icons",
  "workbench.layoutControl.enabled": false,
  "workbench.navigationControl.enabled": false,
  "workbench.preferredDarkColorTheme": "Min Dark",
  "workbench.preferredLightColorTheme": "Min Light",
  "workbench.preferredHighContrastColorTheme": "GitHub Dark High Contrast",
  "workbench.preferredHighContrastLightColorTheme": "GitHub Light High Contrast",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.sideBar.location": "right",
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-f>": false,
    "<C-s>": false,
    "<C-z>": false
  },
  "vim.leader": "space",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.smartRelativeLine": false,
  "vim.foldfix": true,
  "vim.surround": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "a",
        "a"
      ],
      "commands": [
        {
          "command": "alternateFile.alternateFile"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "v"
      ],
      "commands": [
        {
          "command": "alternateFile.alternateFile"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "t",
        "a"
      ],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "mix: Run tests"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "t",
        "f"
      ],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "mix: Run tests in current file"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "t",
        "r"
      ],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "mix: Run test at cursor"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "b"
      ],
      "commands": [
        "workbench.action.showAllEditors"
      ]
    },
    {
      "before": [
        "<leader>",
        "o"
      ],
      "commands": [
        "workbench.view.explorer"
      ]
    },
    {
      "before": [
        "<leader>",
        "h"
      ],
      "commands": [
        "workbench.action.splitEditorDown"
      ]
    },
    {
      "before": [
        "<leader>",
        "v"
      ],
      "commands": [
        "workbench.action.splitEditorRight"
      ]
    },
    {
      "before": [
        "g",
        "d"
      ],
      "commands": [
        "editor.action.revealDefinition"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.goToReferences"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    {
      "before": [
        "<leader>",
        "q"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        ",",
        "<leader>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "f"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "tailwind-fold.autoFold": true,
  "tailwind-fold.supportedLanguages": [
    "html",
    "typescriptreact",
    "javascriptreact",
    "typescript",
    "javascript",
    "vue-html",
    "vue",
    "phoenix-heex",
    "php",
    "markdown",
    "coffeescript",
    "svelte",
    "astro",
    "erb"
  ],
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "cSpell.userWords": [
    "Livecomp",
    "Logflare",
    "vercel"
  ],
  "makefile.configureOnOpen": true,
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "nix.formatterPath": "alejandra",
}