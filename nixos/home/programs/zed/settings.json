{
  "agent": {
    "model_parameters": [],
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4.1"
    },
    "version": "2"
  },
  "vim_mode": true,
  "project_panel": {
    "default_width": 240,
    "dock": "right",
    "entry_spacing": "comfortable",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "auto_reveal_entries": true,
    "auto_fold_dirs": true
  },
  "ui_font_size": 14,
  "buffer_font_size": 18,
  "buffer_font_family": "Zed Plex Mono",
  "buffer_font_features": {
    "calt": false
  },
  "terminal": {
    "font_family": "Zed Plex Mono",
    "font_size": 18
  },
  "languages": {
    "C": { "format_on_save": "on" }
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  }
}
