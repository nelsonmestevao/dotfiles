{
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 22.0,
  "project_panel": {
    "default_width": 240,
    "dock": "right",
    "entry_spacing": "comfortable",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "auto_reveal_entries": true,
    "auto_fold_dirs": true
  },
  "terminal": {
    "font_family": "JetBrainsMono Nerd Font"
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  }
}

